<configuration>
   <runtime>
      <gcServer enabled="true"/>
     <caching enabled="false" />
     <staticContent>
       <clientCache cacheControlMode="DisableCache" />
     </staticContent>
     <httpProtocol>
       <customHeaders>
         <add name="Cache-Control" value="no-cache, no-store" />
         <add name="Pragma" value="no-cache" />
         <add name="Expires" value="-1" />
       </customHeaders>
     </httpProtocol>
   </runtime>
</configuration>
